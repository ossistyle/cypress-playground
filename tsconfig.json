{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "es6"],
    "baseUrl": ".",
    "types": [
      "node",
      "cypress",
      "cypress-fail-fast",
      "cypress-mochawesome-reporter",
      "@bahmutov/cy-grep"
    ],
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "removeComments": true,
    "noEmit": true,
    "outDir": "build",
    "rootDir": "cypress",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "skipLibCheck": false
  },
  "include": [
    "cypress/**/*.ts",
    "node_modules/@bahmutov/cy-grep/src/tags-are-strings.d.ts"
  ],
  "exclude": []
}
