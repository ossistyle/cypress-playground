{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "es6"],
    "baseUrl": ".",
    "types": [
      "node",
      "cypress",
      "cypress-fail-fast",
      "@cypress/grep",
      "cypress-mochawesome-reporter"
    ],
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "removeComments": true,
    "noEmit": true,
    "outDir": "build",
    "rootDir": "cypress",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "skipLibCheck": false
  },
  "include": ["cypress"],
  "exclude": ["node_modules"]
}
